   "source": [
    "def make_train_valid_test_dfs():\n",
    "    df = pd.read_csv(f\"{CFG.captions_path}/dataset.csv\")\n",
    "    image_ids = np.arange(0, len(df))\n",
    "    np.random.seed(42)\n",
    "    valid_size = int(0.2 * len(image_ids))\n",
    "    test_size = int(0.1 * len(image_ids))\n",
    "    test_ids = np.random.choice(image_ids, size=test_size, replace=False)\n",
    "    remaining_ids = [id_ for id_ in image_ids if id_ not in test_ids]\n",
    "    valid_ids = np.random.choice(remaining_ids, size=valid_size, replace=False)\n",
    "    train_ids = [id_ for id_ in remaining_ids if id_ not in valid_ids]\n",
    "    \n",
    "    train_df = df.iloc[train_ids].reset_index(drop=True)\n",
    "    valid_df = df.iloc[valid_ids].reset_index(drop=True)\n",
    "    test_df = df.iloc[test_ids].reset_index(drop=True)\n",
    "    return train_df, valid_df, test_df"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "b1ee7ba3",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-18T08:11:35.720246Z",
     "iopub.status.busy": "2025-06-18T08:11:35.720032Z",
     "iopub.status.idle": "2025-06-18T08:11:35.723903Z",
     "shell.execute_reply": "2025-06-18T08:11:35.723395Z"
    },
    "papermill": {
     "duration": 0.008806,
     "end_time": "2025-06-18T08:11:35.724916",
     "exception": false,
     "start_time": "2025-06-18T08:11:35.716110",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def build_loaders(dataframe, tokenizer, mode):\n",
    "    transforms = get_transforms(mode=mode)\n",
    "    dataset = CLIPDataset(\n",
    "        dataframe[\"   Image\"].values,\n",
    "        dataframe[\"Text\"].values,\n",
    "        dataframe[\"Label\"].values,\n",
    "        tokenizer=tokenizer,\n",
    "        transforms=transforms,\n",
    "    )\n",
    "    dataloader = torch.utils.data.DataLoader(\n",
    "        dataset,\n",
    "        batch_size=CFG.batch_size,\n",
    "        num_workers=CFG.num_workers,\n",
    "        shuffle=True if mode == \"train\" else False,\n",
    "    )\n",
    "    return dataloader"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "e0b785d6",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-18T08:11:35.732375Z",
     "iopub.status.busy": "2025-06-18T08:11:35.732136Z",
     "iopub.status.idle": "2025-06-18T08:11:35.737081Z",
     "shell.execute_reply": "2025-06-18T08:11:35.736610Z"
    },
    "papermill": {
     "duration": 0.009755,
     "end_time": "2025-06-18T08:11:35.738105",
     "exception": false,
     "start_time": "2025-06-18T08:11:35.728350",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def train_epoch(model, train_loader, optimizer, lr_scheduler, step):\n",
    "    loss_meter = AvgMeter()\n",
    "    acc_meter = AvgMeter()\n",
    "    tqdm_object = tqdm(train_loader, total=len(train_loader))\n",
    "    for batch in tqdm_object:\n",
    "        batch = {k: v.to(CFG.device) for k, v in batch.items() if k != \"caption\"}\n",
    "        logits = model(batch)\n",
    "        loss = F.cross_entropy(logits, batch[\"label\"])\n",
    "        optimizer.zero_grad()\n",
    "        loss.backward()\n",
    "        optimizer.step()\n",
    "        if step == \"batch\":\n",
    "            lr_scheduler.step()\n",
    "        \n",
    "        count = batch[\"image\"].size(0)\n",
    "        loss_meter.update(loss.item(), count)\n",
    "        preds = torch.argmax(logits, dim=1)\n",
    "        acc = (preds == batch[\"label\"]).float().mean()\n",
    "        acc_meter.update(acc.item(), count)\n",
    "        \n",
    "        tqdm_object.set_postfix(train_loss=loss_meter.avg, train_acc=acc_meter.avg, lr=get_lr(optimizer))\n",
    "    return loss_meter, acc_meter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "89d279a4",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-18T08:11:35.746389Z",
     "iopub.status.busy": "2025-06-18T08:11:35.745879Z",
     "iopub.status.idle": "2025-06-18T08:11:35.750793Z",
     "shell.execute_reply": "2025-06-18T08:11:35.750118Z"
    },
    "papermill": {
     "duration": 0.009773,
     "end_time": "2025-06-18T08:11:35.751808",
     "exception": false,
     "start_time": "2025-06-18T08:11:35.742035",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def valid_epoch(model, valid_loader):\n",
    "    loss_meter = AvgMeter()\n",
    "    acc_meter = AvgMeter()\n",
    "    tqdm_object = tqdm(valid_loader, total=len(valid_loader))\n",
    "    for batch in tqdm_object:\n",
    "        batch = {k: v.to(CFG.device) for k, v in batch.items() if k != \"caption\"}\n",
    "        logits = model(batch)\n",
    "        loss = F.cross_entropy(logits, batch[\"label\"])\n",
    "        \n",
    "        count = batch[\"image\"].size(0)\n",
    "        loss_meter.update(loss.item(), count)\n",
    "        preds = torch.argmax(logits, dim=1)\n",
    "        acc = (preds == batch[\"label\"]).float().mean()\n",
    "        acc_meter.update(acc.item(), count)\n",
    "        \n",
    "        tqdm_object.set_postfix(valid_loss=loss_meter.avg, valid_acc=acc_meter.avg)\n",
    "    return loss_meter, acc_meter\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "a3b13692",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-18T08:11:35.759630Z",
     "iopub.status.busy": "2025-06-18T08:11:35.759016Z",
     "iopub.status.idle": "2025-06-18T08:11:35.763670Z",
     "shell.execute_reply": "2025-06-18T08:11:35.763201Z"
    },
    "papermill": {
     "duration": 0.00941,
     "end_time": "2025-06-18T08:11:35.764653",
     "exception": false,
     "start_time": "2025-06-18T08:11:35.755243",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def test_epoch(model, test_loader):\n",
    "    acc_meter = AvgMeter()\n",
    "    all_preds = []\n",
    "    all_labels = []\n",
    "    tqdm_object = tqdm(test_loader, total=len(test_loader))\n",
    "    for batch in tqdm_object:\n",
    "        batch = {k: v.to(CFG.device) for k, v in batch.items() if k != \"caption\"}\n",
    "        logits = model(batch)\n",
    "        preds = torch.argmax(logits, dim=1)\n",
    "        acc = (preds == batch[\"label\"]).float().mean()\n",
    "        acc_meter.update(acc.item(), batch[\"image\"].size(0))\n",
    "        all_preds.extend(preds.cpu().numpy())\n",
    "        all_labels.extend(batch[\"label\"].cpu().numpy())\n",
    "        tqdm_object.set_postfix(test_acc=acc_meter.avg)\n",
    "    return acc_meter, all_preds, all_labels\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "2948cf98",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-18T08:11:35.772251Z",
     "iopub.status.busy": "2025-06-18T08:11:35.772032Z",
     "iopub.status.idle": "2025-06-18T08:11:35.778736Z",
     "shell.execute_reply": "2025-06-18T08:11:35.778075Z"
    },
    "papermill": {
     "duration": 0.011614,
     "end_time": "2025-06-18T08:11:35.779728",
     "exception": false,
     "start_time": "2025-06-18T08:11:35.768114",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "def main():\n",
    "    train_df, valid_df, test_df = make_train_valid_test_dfs()\n",
    "    tokenizer = XLMRobertaTokenizer.from_pretrained(CFG.text_tokenizer)\n",
    "    train_loader = build_loaders(train_df, tokenizer, mode=\"train\")\n",
    "    valid_loader = build_loaders(valid_df, tokenizer, mode=\"valid\")\n",
    "    test_loader = build_loaders(test_df, tokenizer, mode=\"test\")\n",
    "\n",
    "    model = CLIPClassifier().to(CFG.device)\n",
    "    params = [\n",
    "        {\"params\": model.image_encoder.parameters(), \"lr\": CFG.image_encoder_lr},\n",
    "        {\"params\": model.text_encoder.parameters(), \"lr\": CFG.text_encoder_lr},\n",
    "        {\"params\": itertools.chain(\n",
    "            model.image_projection.parameters(),\n",
    "            model.text_projection.parameters(),\n",
    "            model.classifier.parameters()\n",
    "        ), \"lr\": CFG.head_lr, \"weight_decay\": CFG.weight_decay}\n",
    "    ]\n",
    "    optimizer = torch.optim.AdamW(params, weight_decay=0.)\n",
    "    lr_scheduler = torch.optim.lr_scheduler.ReduceLROnPlateau(\n",
    "        optimizer, mode=\"min\", patience=CFG.patience, factor=CFG.factor\n",
    "    )\n",
    "\n",
    "    best_loss = float('inf')\n",
    "    for epoch in range(CFG.epochs):\n",
    "        print(f\"Epoch: {epoch + 1}\")\n",
    "        model.train()\n",
    "        train_loss, train_acc = train_epoch(model, train_loader, optimizer, lr_scheduler, \"epoch\")\n",
    "        model.eval()\n",
    "        with torch.no_grad():\n",
    "            valid_loss, valid_acc = valid_epoch(model, valid_loader)\n",
    "        \n",
    "        if valid_loss.avg < best_loss:\n",
    "            best_loss = valid_loss.avg\n",
    "            torch.save(model.state_dict(), \"best.pt\")\n",
    "            print(f\"Saved Best Model! Valid Loss: {valid_loss.avg:.4f}, Valid Acc: {valid_acc.avg:.4f}\")\n",
    "        \n",
    "        lr_scheduler.step(valid_loss.avg)\n",
    "\n",
    "    # Test evaluation\n",
    "    model.load_state_dict(torch.load(\"best.pt\", map_location=CFG.device))\n",
    "    model.eval()\n",
    "    with torch.no_grad():\n",
    "        test_acc, test_preds, test_labels = test_epoch(model, test_loader)\n",
    "    print(f\"Final Test Accuracy: {test_acc.avg:.4f}\")\n"
   ]
  },
