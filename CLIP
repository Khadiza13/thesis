   },
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "/tmp/ipykernel_19/3729451583.py:83: UserWarning: Argument(s) 'always_apply' are not valid for transform Resize\n",
      "  A.Resize(CFG.size, CFG.size, always_apply=True),\n",
      "/tmp/ipykernel_19/3729451583.py:84: UserWarning: Argument(s) 'always_apply' are not valid for transform Normalize\n",
      "  A.Normalize(max_pixel_value=255.0, always_apply=True),\n"
     ]
