    "# Example usage for single prediction\n",
    "model = CLIPClassifier().to(CFG.device)\n",
    "model.load_state_dict(torch.load(\"best.pt\", map_location=CFG.device))\n",
    "tokenizer = XLMRobertaTokenizer.from_pretrained(CFG.text_tokenizer)\n",
    "transform = get_transforms(mode=\"valid\")\n",
    "prediction = predict_single(model, f\"{CFG.image_path}/1.png\", \"তার স্বামী, আমি খুব খুশি যে আমি সে নই।\", tokenizer, transform)\n",
